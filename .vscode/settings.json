{
    "editor.rulers": [80],
    "cortex-debug.JLinkGDBServerPath": "D:/WINDOWS_DATA/Folder Files/SEGGER/JLink/JLinkGDBServerCL.exe",
    "cmake.buildDirectory": "${workspaceRoot}/build",
    "cmake.buildArgs": [
        "--parallel"
